			</column>
			<column label="clock.rate" sf="6" width="12">
				<parameter idref="clock.rate"/>
			</column>
		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="10000" fileName="s1.log" overwrite="false">
			<posterior idref="posterior"/>
